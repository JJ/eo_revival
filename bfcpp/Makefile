CXX=g++ 
prefix=..
CXXFLAGS=-Wall -O4

###############################################################################

SRC = bfcpp.cpp
BIN = bfcpp

###############################################################################

all:	${BIN}

clean:	
	rm -f ${BIN} *~ core

bfcpp:	
	g++ -Wall -Wno-deprecated -std=c++11 bfcpp.cpp -o bfcpp

###############################################################################

.NOEXPORT:
